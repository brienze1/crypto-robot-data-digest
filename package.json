{
  "name": "typescript-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild:production": "npm run clean && npm run lint && npm run test",
    "prebuild:localstack": "npm run clean && npm run lint && npm run test",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "build:production": "cross-env NODE_ENV=production babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "build:localstack": "cross-env NODE_ENV=localstack babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "build": "cross-env NODE_ENV=development babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "lint": "eslint --fix --ext .ts .",
    "dev": "ts-node -r tsconfig-paths/register -r dotenv/config src/test/local/LocalTest.test.ts dotenv_config_path=resources/.env.development dotenv_config_debug=true",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brienze1/typescript-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brienze1/typescript-template/issues"
  },
  "homepage": "https://github.com/brienze1/typescript-template#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/aws-lambda": "^8.10.101",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.2.6",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  }
